<section class="p-3">
    <form id="registerForm" class="p-3">
    
        <div class="mb-3">
            <label for="firstName" class="form-label">First Name</label>
            <input type="text" class="form-control" id="firstName" name="firstName" aria-describedby="nameHelp">
            <div id="nameHelp" class="form-text">My name is...</div>
        </div>

        <div class="mb-3">
            <label for="lastName" class="form-label">Last Name</label>
            <input type="text" class="form-control" id="lastName" name="lastName" aria-describedby="lastNameHelp">
            <div id="nameHelp" class="form-text">My last name is...</div>
        </div>

        <div class="mb-3">
            <label for="exampleInputEmail1" class="form-label">Email address</label>
            <input type="email" class="form-control" id="exampleInputEmail1" name="regEmail" aria-describedby="emailHelp">
            <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
        </div>
        
        <div class="mb-3">
            <label for="age" class="form-label">Age</label>
            <input type="number" class="form-control" id="age" name="age" aria-describedby="ageHelp">
            <div id="ageHelp" class="form-text">30</div>
        </div>

        <div class="mb-3">
            <label for="exampleInputPassword1" class="form-label">Password</label>
            <input type="password" class="form-control" id="registerPassword" name="regPassword">
        </div>

        <button type="submit" class="btn btn-primary">Submit</button>
    </form>

    <h5>Do you have an account? <a href="/web/login/" style="color: blue;"> >>>Login</a></h1>
</section>

<script src="/js/register.js"></script>